{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "{{ ID }}",
  "title": "Person Schema",
  "description": "Schema for Person validation",
  "type": "object",
  "properties": {
    "numTeeth": {
      "type": "integer",
      "minimum": 0,
      "maximum": 32,
      "description": "How many teeth a person has"
    },
    "firstName": {
      "type": "string",
      "description": "The person's first name"
    }
  },
  "required": [
    "numTeeth",
    "firstName"
  ]
}